{
  "name": "expo-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 bunx expo start --localhost",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "expo:start:static:build": "bunx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "db:push": "bunx drizzle-kit push",
    "start": "bunx expo start",
    "web": "bunx expo start --web",
    "lint": "bunx biome check --fix .",
    "format": "bunx biome format --write .",
    "biome": "bunx biome check .",
    "biome:fix": "bunx biome check --write .",
    "biome:ci": "bunx biome ci .",
    "typecheck": "bunx tsc --noEmit",
    "check:ast": "bun scripts/check-ast.ts",
    "coverage": "bun test features shared --coverage --coverage-reporter=text --coverage-reporter=lcov --coverage-dir=coverage",
    "check:coverage": "bun run coverage && bun scripts/check-coverage.ts 80 80",
    "check": "bunx biome check . && bun run typecheck && bun run check:ast",
    "knip": "bunx knip",
    "depcruise": "bunx depcruise --config .dependency-cruiser.js app features shared server",
    "audit": "bun audit",
    "e2e": "bunx playwright test",
    "a11y": "bunx playwright test -g @a11y",
    "size": "bunx size-limit",
    "perf": "bunx lhci autorun",
    "verify": "bun run biome:ci && bun run typecheck && bun run check:ast && bun run knip && bun run depcruise && bun run audit",
    "verify:full": "bun run verify && bun run check:coverage && bun run e2e && bun run a11y && bun run size && bun run perf"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/react-query": "^5.90.21",
    "@ungap/structured-clone": "^1.3.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "expo": "~54.0.33",
    "expo-audio": "^1.1.1",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-font": "~14.0.11",
    "expo-glass-effect": "~0.1.9",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "^15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "express": "^5.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1",
    "tsx": "^4.21.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@babel/core": "^7.29.0",
    "@biomejs/biome": "^2.4.4",
    "@expo/ngrok": "^4.1.3",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.58.2",
    "@size-limit/file": "^12.0.0",
    "@types/bun": "^1.3.9",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.14",
    "babel-plugin-react-compiler": "^1.0.0",
    "dependency-cruiser": "^17.3.8",
    "drizzle-kit": "^0.31.9",
    "knip": "^5.84.1",
    "lefthook": "^2.1.1",
    "size-limit": "^12.0.0",
    "esbuild": "^0.25.1",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "minimatch": ">=10.2.1",
    "tmp": ">=0.2.4",
    "esbuild": ">=0.25.1"
  },
  "size-limit": [
    {
      "path": "server/index.ts",
      "limit": "10 kB"
    },
    {
      "path": "shared/api/subsonic/subsonic.ts",
      "limit": "15 kB"
    }
  ],
  "private": true
}
